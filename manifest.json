{
  "manifest_version": 2,

  "name": "Ctrl-F++",
  "description": "This extension searches webpage source code for useful examples",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["http://www.google.com/*","https://www.google.com/*"],
      "js": ["myscript.js"]
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
