{
  "manifest_version": 2,

  "name": "Ctrl-F++",
  "description": "A Chrome extension for students of HTML.",
  "version": "1.1",

 "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["jquery-2.1.3.min.js", "content.js"]
  }],

  "browser_action": {
    "default_icon": "spyglass.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "24": "spyglass.png",
    "48": "spyglass.png",
    "128": "spyglass.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}
